<% if user_signed_in? %>
<div class='container'>
  <div class='row'>
    <div class='col-md-8 offset-md-4'>
     <div class="search_form">
      <%= form_with url: search_path, local: true, method: :get do |f| %>
        <%= f.text_field :keyword %>
        <%= f.submit "タグ検索", class: "btn btn-primary" %>
       <% end %>
     </div>
   <% end %>
    </div>
  </div>
</div>